{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "types": [
      "@cloudflare/workers-types/2023-07-01"
    ],
    "baseUrl": "./",
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@env/*": [
        "src/environments/*"
      ],
      "@core/*": [
        "src/app/core/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@modules/*": [
        "src/app/modules/*"
      ],
      "@pages/*": [
        "src/app/modules/pages/*"
      ],
      "@services/*": [
        "src/app/services/*"
      ],
      "@models/*": [
        "src/app/models/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/app/shared/components/*"
      ],
      "@utils/*": [
        "src/app/shared/utils/*"
      ],
      "@spartan-ng/ui-table-helm": [
        ".\\libs/ui/ui-table-helm/src/index.ts"
      ],
      "@spartan-ng/ui-accordion-helm": [
        ".\\libs/ui/ui-accordion-helm/src/index.ts"
      ],
      "@spartan-ng/ui-icon-helm": [
        ".\\libs/ui/ui-icon-helm/src/index.ts"
      ],
      "@spartan-ng/ui-button-helm": [
        ".\\libs/ui/ui-button-helm/src/index.ts"
      ],
      "@spartan-ng/ui-formfield-helm": [
        ".\\libs/ui/ui-formfield-helm/src/index.ts"
      ],
      "@spartan-ng/ui-input-helm": [
        ".\\libs/ui/ui-input-helm/src/index.ts"
      ],
      "@spartan-ng/ui-checkbox-helm": [
        ".\\libs/ui/ui-checkbox-helm/src/index.ts"
      ],
      "@spartan-ng/ui-menu-helm": [
        ".\\libs/ui/ui-menu-helm/src/index.ts"
      ],
      "@spartan-ng/ui-select-helm": [
        ".\\libs/ui/ui-select-helm/src/index.ts"
      ],
      "@spartan-ng/ui-dialog-helm": [
        ".\\libs/ui/ui-dialog-helm/src/index.ts"
      ],
      "@spartan-ng/ui-label-helm": [
        ".\\libs/ui/ui-label-helm/src/index.ts"
      ],
      "@spartan-ng/ui-badge-helm": [
        ".\\libs/ui/ui-badge-helm/src/index.ts"
      ],
      "@spartan-ng/ui-command-helm": [
        ".\\libs/ui/ui-command-helm/src/index.ts"
      ],
      "@spartan-ng/ui-popover-helm": [
        ".\\libs/ui/ui-popover-helm/src/index.ts"
      ],
      "@spartan-ng/ui-avatar-helm": [
        ".\\libs/ui/ui-avatar-helm/src/index.ts"
      ],
      "@spartan-ng/ui-breadcrumb-helm": [
        ".\\libs/ui/ui-breadcrumb-helm/src/index.ts"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
